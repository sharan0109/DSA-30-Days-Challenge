class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        vector<int> ans;

        // First time nums add karo
        for(int i = 0; i < nums.size(); i++) {
            ans.push_back(nums[i]);
        }

        // Second time nums add karo
        for(int i = 0; i < nums.size(); i++) {
            ans.push_back(nums[i]);
        }

        return ans;
    }
};
